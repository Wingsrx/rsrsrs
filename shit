	--Keybinds
	UIS.InputBegan:connect(function(io,gpe)
		if(gpe or Attack)then return end
		
		--MODES
		if(io.KeyCode == Enum.KeyCode.One and Mode~='Rainbow Mode')then 
			changeMode'Rainbow Mode' VaporTaunt()
