		elseif Mode == "Cyberpunk Mode" then Chat("Welcome to the future!")
		elseif Mode == "Galaxy Mode" then Chat("Cosmic power flows through me!")
		elseif Mode == "Neon Mode" then Chat("Electric vibes activated!")
		elseif Mode == "Fire Mode" then Chat("Burning with fury!")
		elseif Mode == "Ice Mode" then Chat("Frozen in time!")
		elseif Mode == "Lightning Mode" then Chat("Thunder and lightning!")
		elseif Mode == "Shadow Mode" then Chat("Darkness consumes all!")
		elseif Mode == "Cosmic Mode" then Chat("The universe is mine!")
		elseif Mode == "Prismatic Mode" then Chat("All colors, one power!")
		elseif Mode == "Void Mode" then Chat("Nothingness awaits!")
		elseif Mode == "Solar Mode" then Chat("The sun's power!")
		elseif Mode == "Lunar Mode" then Chat("Moonlight guides me!")
		elseif Mode == "Plasma Mode" then Chat("Energy unleashed!")
		elseif Mode == "Crystal Mode" then Chat("Crystalline perfection!")
		elseif Mode == "Storm Mode" then Chat("Weather under control!")
		elseif Mode == "Phoenix Mode" then Chat("Rising from ashes!")
		elseif Mode == "Dragon Mode" then Chat("Ancient power awakens!")
		elseif Mode == "Chaos Mode" then Chat("Chaos reigns!")
		elseif Mode == "Order Mode" then Chat("Perfect balance!")
		elseif Mode == "Infinity Mode" then Chat("Endless possibilities!")
		elseif Mode == "Quantum Mode" then Chat("Reality bends to my will!")
		elseif Mode == "Time Mode" then Chat("Time is mine to control!")
		elseif Mode == "Dream Mode" then Chat("Dreams become reality!")
		elseif Mode == "Nightmare Mode" then Chat("Fear is power!")
		elseif Mode == "Speed Mode" then Chat("Faster than light!")
		elseif Mode == "Power Mode" then Chat("Maximum strength!")
		elseif Mode == "Magic Mode" then Chat("Mystical forces!")
		elseif Mode == "Tech Mode" then Chat("Technology supreme!")
