		{Name="Ultra Rainbow",Walkspeed=60,moveVal=18,Font=Enum.Font.Arcade,StrokeColor=C3.N(.5,.5,.5),Music=2415462372,LeftWing={0,BrickColor.new'Bright red'.Color,Enum.Material.Neon},RightWing={0,BrickColor.new'Bright blue'.Color,Enum.Material.Neon},WingAnim='UltraRainbow'},
		{Name="Matrix Mode",Walkspeed=45,moveVal=12,Font=Enum.Font.Ubuntu,StrokeColor=C3.RGB(0,255,0),Music=1091263121,LeftWing={0,BrickColor.new'Lime green'.Color,Enum.Material.Neon},RightWing={0,BrickColor.new'Bright green'.Color,Enum.Material.Neon},WingAnim='Matrix'},
		{Name="Supernova",Walkspeed=90,moveVal=15,Font=Enum.Font.Sarpanch,StrokeColor=C3.RGB(255,200,0),Music=1837185092,LeftWing={0,BrickColor.new'Bright yellow'.Color,Enum.Material.Neon},RightWing={0,BrickColor.new'Bright orange'.Color,Enum.Material.Neon},WingAnim='Supernova'},
		{Name="Absolute Zero",Walkspeed=15,moveVal=4,Font=Enum.Font.Gotham,StrokeColor=C3.RGB(200,255,255),Music=214902446,LeftWing={0,BrickColor.new'Pastel light blue'.Color,Enum.Material.Glass},RightWing={0,BrickColor.new'White'.Color,Enum.Material.Glass},WingAnim='AbsoluteZero'},
		{Name="Hellfire",Walkspeed=70,moveVal=18,Font=Enum.Font.Creepster,StrokeColor=C3.RGB(255,50,0),Music=6059390355,LeftWing={0,BrickColor.new'Really red'.Color,Enum.Material.Neon},RightWing={0,BrickColor.new'Bright orange'.Color,Enum.Material.Neon},WingAnim='Hellfire'},
	}

	--Helper functions
	function NewInstance(instance,parent,properties)
		local inst = Instance.new(instance)
		inst.Parent = parent
		if(properties)then
			for i,v in next, properties do
				pcall(function() inst[i] = v end)
			end
		end
		return inst
	end

	function newMotor(P0,P1,C0,C1)
		return NewInstance('Motor',P0,{Part0=P0,Part1=P1,C0=C0,C1=C1})
	end

	local welds = {}
	local WeldDefaults = {}
	table.insert(welds,newMotor(Torso,Head,CF.N(0,1.5,0),CF.N()))
	table.insert(welds,newMotor(Root,Torso,CF.N(),CF.N()))
	table.insert(welds,newMotor(Torso,RLeg,CF.N(.5,-1,0),CF.N(0,1,0)))
	table.insert(welds,newMotor(Torso,RArm,CF.N(1.5,.5,0),CF.N(0,.5,0)))
	table.insert(welds,newMotor(Torso,LLeg,CF.N(-.5,-1,0),CF.N(0,1,0)))
	table.insert(welds,newMotor(Torso,LArm,CF.N(-1.5,.5,0),CF.N(0,.5,0)))

	WeldDefaults={}
	for i = 1,#welds do
		local v=welds[i]
		WeldDefaults[i]=v.C0
	end

	local NK,RJ,RH,RS,LH,LS=unpack(welds)
	local NKC0,RJC0,RHC0,RSC0,LHC0,LSC0=unpack(WeldDefaults)

	function makeMusic(id,pit,timePos)
		local sound = Torso:FindFirstChild(Player.Name.."song") or Char:FindFirstChild(Player.Name.."song")
		local parent = (MusicMode==2 and Char or Torso)
		if(not sound)then 
			sound = NewInstance("Sound",parent,{Name=Player.Name.."song",Volume=(MusicMode==3 and 0 or 5),Pitch=(pit or 1),Looped=true})
			NewInstance("EqualizerSoundEffect",sound,{HighGain=0,MidGain=2,LowGain=10})
		end
		if(id=='stop')then
			if(sound)then
				sound:Stop()
			end
		else
			local timePos = typeof(timePos)=='number' and timePos or sound.TimePosition
			sound.Volume = (MusicMode==3 and 0 or 5)
			sound.Name = Player.Name.."song"
			sound.Looped=true
			sound.SoundId = "rbxassetid://"..id
			sound.Pitch=(pit or 1)
			sound:Play()
			sound.TimePosition = timePos
		end
		return sound
	end

	function playMusic(id,pitch,timePos)
		return makeMusic(id,pitch,timePos)
	end

	for _,v in next, Hum:GetPlayingAnimationTracks() do
		v:Stop(0)
	end

	function swait(num)
		if num == 0 or num == nil then
			game:service'RunService'.RenderStepped:wait()
		else
			for i = 0, num do
				game:service'RunService'.RenderStepped:wait()
			end
		end
	end

	function Tween(obj,props,time,easing,direction,repeats,backwards)
		easing = easing or Enum.EasingStyle.Quad
		direction = direction or Enum.EasingDirection.Out
		time = time or .5
		repeats = repeats or 0
		backwards = backwards or false
		game:service'TweenService':Create(obj,TweenInfo.new(time,easing,direction,repeats,backwards),props):Play()
	end

	function ShowDamage(Pos, Text, Time, Color)
		local Part = NewInstance("Part",EffectFolder,{
			Transparency = 1,
			Size = Vector3.new(1,1,1),
			Anchored = true,
			CanCollide = false,
			CFrame = CF.N(Pos)
		})
		local Gui = NewInstance("BillboardGui",Part,{
			Size = UDim2.new(3,0,3,0),
			Adornee = Part,
		})
		local Txt = NewInstance("TextLabel",Gui,{
			Size = UDim2.new(1,0,1,0),
			BackgroundTransparency = 1,
			Text = Text,
			TextColor3 = Color or Color3.new(1,1,1),
			TextStrokeTransparency = 0,
			TextScaled = true,
			Font = Enum.Font.GothamBold,
		})
		game:service'Debris':AddItem(Part,Time or 2)
		Tween(Txt,{TextTransparency = 1,TextStrokeTransparency = 1},Time or 2)
		delay(0, function()
			for i = 0, 60*(Time or 2) do
				swait()
				Part.CFrame = Part.CFrame*CF.N(0,.02,0)
			end
		end)
	end

	function Soond(parent,id,pitch,volume,looped,effect,autoPlay)
		local Sound = NewInstance("Sound",parent,{
			SoundId = "rbxassetid://"..id,
			Pitch = pitch or 1,
			Volume = volume or 1,
			Looped = looped or false
		})
		if(effect)then
			NewInstance("ReverbSoundEffect",Sound,{ReverbType=Enum.ReverbType.Plains})
		end
		if(autoPlay)then
			Sound:Play()
		end
		coroutine.wrap(function()
			if(looped)then
				wait()
			else
				wait(Sound.TimeLength)
			end
			Sound:Destroy()
		end)()
		return Sound
	end

	function Part(parent,color,material,size,cframe,anchored,cancollide)
		local Part = NewInstance("Part",parent,{
			BrickColor = BrickColor.new(color),
			Material = material,
			Size = size,
			CFrame = cframe,
			Anchored = anchored,
			CanCollide = cancollide
		})
		return Part
	end

	function Weld(part0,part1,c0,c1)
		local Weld = NewInstance("Weld",part0,{
			Part0 = part0,
			Part1 = part1,
			C0 = c0 or CF.N(),
			C1 = c1 or CF.N()
		})
		return Weld
	end

	function Mesh(parent,meshtype,meshid,textid,scale,offset)
		local Mesh = NewInstance("SpecialMesh",parent,{
			MeshType = meshtype,
			MeshId = meshid or "",
			TextureId = textid or "",
			Scale = scale or V3.N(1,1,1),
			Offset = offset or V3.N(0,0,0)
		})
		return Mesh
	end

	function GotEffect(data)
		local Effect = Part(EffectFolder,data.Color or "White",data.Material or Enum.Material.Neon,data.Size or V3.N(1,1,1),data.CFrame or CF.N(),true,false)
		if(data.Mesh)then
			Mesh(Effect,data.Mesh.Type or Enum.MeshType.Sphere,data.Mesh.MeshId,data.Mesh.TextureId,data.Mesh.Scale,data.Mesh.Offset)
		end
		if(data.Transparency)then
			Effect.Transparency = data.Transparency[1] or 0
		end
		coroutine.wrap(function()
			for i = 0, 1, (1/data.Lifetime or 1) do
				swait()
				if(data.Transparency)then
					Effect.Transparency = M.C(i)*(data.Transparency[2] or 1) + (data.Transparency[1] or 0)
				end
				if(data.Size)then
					Effect.Size = Effect.Size:lerp(data.EndSize or V3.N(0,0,0),.1)
				end
				if(data.CFrame)then
					Effect.CFrame = Effect.CFrame*CF.A(M.RRNG(-5,5),M.RRNG(-5,5),M.RRNG(-5,5))
				end
			end
			Effect:Destroy()
		end)()
		return Effect
	end

	function getMode(modeName)
		for i,v in next, modeInfo do
			if(v.Name==modeName)then
				return v
			end
		end
		return modeInfo[1]
	end

	function changeMudo(modeName)
		local info = getMode(modeName)
		Mode=info.Name
		WalkSpeed=info.Walkspeed
		Change=info.moveVal
		PrimaryColor = info.LeftWing[2]
		if(info.Music and info.Music~=0)then
			music = playMusic(info.Music,info.Pitch or 1)
		elseif(music)then
			music:Stop()
		end
	end

	function changeMode(modeName)
		changeMudo(modeName)
	end

	function Chat(text)
		game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(text)
	end

	function VaporTaunt()
